filetype on
filetype plugin indent on
filetype plugin on

syntax on

set autoindent
set background=dark
set clipboard=unnamed
set clipboard^=unnamedplus
set completeopt-=preview
set cursorline
set expandtab
set hidden
set hlsearch
set ignorecase
set incsearch
set laststatus=2
set noshowmode
set noswapfile
set number
set scrolloff=8
set shiftwidth=4
set showcmd
set sidescrolloff=10
set smartcase
set smarttab
set softtabstop=4
set timeoutlen=1000
set ttimeoutlen=0
